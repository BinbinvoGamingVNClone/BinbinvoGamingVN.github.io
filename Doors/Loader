local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
OrionLib:MakeNotification({
	Name = "BinbinvoGaming Hub",
	Content = "Welcome To BinbinvoGaming Hub",
	Image = "rbxassetid://4483345998",
	Time = 5
})
local Window = OrionLib:MakeWindow({Name = "BinbinvoGaming Hub - Select Floor", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "Doors Floors 2",
	Callback = function()
      		loadstring(game:HttpGet("https://BinbinvoGaming.github.io/Doors/2/Loader",true))()
  	end    
})
Tab:AddButton({
	Name = "Doors Floors 1",
	Callback = function()
      		loadstring(game:HttpGet("https://BinbinvoGaming.github.io/Doors/1/Loader",true))()
  	end    
})
